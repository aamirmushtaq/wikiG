		<script type="text/javascript">
		    function readArticleNames(){
		        file = fopen(getScriptPath("/home/jesal/Flask/templates/all_article_names.txt"),0);
		        if (file!=-1)
		        {
		            file_length = flength(file);
		            content = fread(file,file_length);
		            fclose(file);
		            write(content);
		        }
		        else
		            alert("error!");
		        //document.getElementById("right-container").innerText = content;
		        //write(content);
		    }
        </script>
		
